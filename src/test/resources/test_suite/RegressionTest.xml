<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TTAF - RegressionTest in Android Device">
    <listeners>
        <!--
        This TestNG listener takes care of setting up and tearing down the Appium driver.
    -->
        <listener class-name="com.automation.qa.ttafmobilecore.listener.WebDriverListener"/>
    </listeners>

    <test name="Emulator-1">
        <parameter name="appium-server-port" value="1234"/>
        <parameter name="mobile-device-id" value="emulator-5554"/>
        <parameter name="mobile-version" value="4.4.2"/>
        <parameter name="browserName" value="Chrome"/>
        <parameter name="mobile-device-name" value="Nexus_4_API_19"/>
        <classes>
            <class name="runner.RegressionTest"/>
        </classes>
    </test>
</suite>